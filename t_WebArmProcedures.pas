    empl:= Cache.arEmplInfo[puserID];
 empl:= Cache.arEmplInfo[80];   //!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! 
//--------------------------------------------------- отрабатываем запись в базу
