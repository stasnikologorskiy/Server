       HTTP.ProxyParams.ProxyUsername:=pIniFile.ReadString('Proxy', 'login', '');
       HTTP.ProxyParams.ProxyPassword:=pIniFile.ReadString('Proxy', 'Password', '');
     end;
     SSLIOHandlerSocketOpenSSL:= TIdSSLIOHandlerSocketOpenSSL.Create(nil);
