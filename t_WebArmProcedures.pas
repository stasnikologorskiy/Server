function fnRepRep(UserID: integer; var lstBodyMail: TStringList; var FName, Subj,     // 66 REpRep
   ContentType: string; ThreadData: TThreadData; filter_data: string): string;   
const
 nmProc = 'fnRepRep'; // имя процедуры/функции
var 
    IBDatabase, LogIBDatabase: TIBDatabase;
    IBSQL, IBSQLEmpl, LogIBSQL: TIBSQL;
    FilterData: TStringList;
    DateStart, DateEnd: TDatetime;   
    iCount: integer;
    MsgStr: tas;
    flSUF: boolean;
    Pool: TIBCntsPool;
//    file_csv: textfile;
    Content, PathFile, Title, grpName: string;
    Percent: real;
    text, textEmpl, TextCount, TypeImp: string;
    IMPNAME, REPORT, IMPORT, ROLENAME: string;
    IMPCODE, ROLECODE, countI, countR: integer;
    CellStylesArray: TXmlCellStylesArray; //Массив стилей оформления документа
    HeaderStyle, NameStyle, BoldHeaderStyle, NumberStyle, ItKolStyle, KolStyle, BoldStyleL, BoldStyleR, BoldStyleC: TXmlReportStyle;  //Стили оформления документа
    XL: TExcelApplication;
    WorkBook: Excel_TLB._Workbook;
    WorkSheet: Excel_TLB._Worksheet;
    usedRng: Range;
    SheetCount,SheetID, EMRLCODE: integer;
    SheetName, EMPLName, s: string;
    i, j, k: integer;
    SLRole: TStringList;
begin
  text:= 'select 0 IMTPCODE, "" as IMTPNAME, "" as IMTPREPORT, "" as IMTPIMPORT, max(IMTPCODE) as ROLECODE, "" as ROLENAME from IMPORTTYPES'#13#10
       + 'union '#13#10
       + 'SELECT IMTPCODE, IMTPNAME, IMTPREPORT, IMTPIMPORT,ROLECODE, ROLENAME'#13#10
       + 'FROM IMPORTTYPES'#13#10
       + 'left join LINKIMPTYPEROLE on IMTPCODE= LITRIMTPCODE'#13#10
       + 'left join ROLES on LITRROLECODE= ROLECODE'#13#10
       + 'order by 1,5 --IMTPCODE, ROLECODE';
  textEmpl:= 'select EMRLEMPLCODE FROM EMPLOYEESROLES where EMRLROLECODE= :pEMRLROLECODE ';
  textCount:= 'select count(thlgcommand) countQ from LOGTHREADS'#13#10
            + 'where  THLGCOMMAND= :pTHLGCOMMAND and THLGBEGINTIME>= :pDateBeg and THLGBEGINTIME<= :pDateEnd and THLGPARAMS containing "ImpTypeId= ';
  FilterData:=nil;
  s:= '';
  iCount:= 0;
  IMPCODE:= 0;
  ROLECODE:= 0;
  DateStart:= 0;
  DateEnd:= 0;
  FilterData:= TStringList.Create;
  SetLength(MsgStr, 0); 
  LogIBDatabase:= nil;
  LogIBSQL:= nil;
  IBDatabase:= nil;
  IBSQL:= nil;
  IBSQLEmpl:= nil;
  Pool:= nil;
  SLRole:= TStringList.Create;
  try try
    FilterData.Text:= filter_data;
    DateStart:= StrToDateTime(FilterData.Values['dop_fromdate']); // получаем DateStart
    DateEnd:= StrToDateTime(FilterData.Values['dop_todate']);
    if date()-DateStart>731 then begin
      s:='Укажите более свежую дату начала периода.';
//      raise EBOBError.Create('Oтчет по использованию подбора по атрибутам.'+#13+s);
      exit;
    end;
    if not GetEmplTmpFilePath(UserID, PathFile, s) then raise EBOBError.Create(s);
    if FName='' then begin
      if not GetEmplTmpFilePath(UserID, PathFile, s) then raise EBOBError.Create(s); 
        FName:= PathFile+fnFormRepFileName('66', '.xlsx', ImpCheck.GetCheckKind(UserID, ThreadData.ID));
    end;
    if FileExists(FName) and not SysUtils.DeleteFile(FName) then begin
      s:= MessText(mtkNotDelPrevFile);
      raise EBOBError.Create(MessText(mtkNotDelPrevFile));
    end;
    flSUF:= (DateEnd < Date) and cntsSUFLOG.BaseConnected; // если до вчера - берем с суфлера
    if flSUF then Pool:= cntsSufLOG else Pool:= cntsLOG;
    try
      LogIBDatabase:= Pool.GetFreeCnt;
    except
      exit;
    end;
    LogIBSQL:= fnCreateNewIBSQL(LogIBDatabase,'LogQuery_'+nmProc, ThreadData.ID, tpRead, true);
    LogIBSQL.SQL.Text:= textCount;
    IBDatabase:= cntsORD.GetFreeCnt;
    IBSQL:= fnCreateNewIBSQL(IBDatabase, 'IBSQL_'+nmProc, ThreadData.ID, tpRead, true);
    IBSQL.SQL.Text:= text;
    IBSQLEmpl:= fnCreateNewIBSQL(IBDatabase, 'IBSQL_'+nmProc, ThreadData.ID, tpRead, true);
    IBSQLEmpl.SQL.Text:= textEmpl;
//'SELECT IMTPCODE, IMTPNAME, IMTPREPORT, IMTPIMPORT,ROLECODE, ROLENAME'    
    ImpCheck.SetProcessPercent(UserId,ThreadData.ID,5);
    IBSQL.ExecQuery;

    XL:= TExcelApplication.Create(nil);
    WorkBook:= nil;
    CoInitializeEx(nil, COINIT_MULTITHREADED); //--== Инициализируем работу экземпляра Excel
    XL.ConnectKind:= ckNewInstance;
    XL.Connect;
    XL.Visible[lcid]:= False;
    XL.ScreenUpdating[lcid]:= False;
    XL.DisplayAlerts[lcid]:= False;
    XL.Calculation[lcid]:= xlManual;
    with XL.ErrorCheckingOptions do begin  // отменяем автоматическую проверку ошибок в ячейках
      BackgroundChecking:= False;
      NumberAsText:= False;
      InconsistentFormula:= False;
    end;

    if Assigned(XL) and (not Assigned(WorkBook)) then begin // создаем файл
      XL.Workbooks.Add(EmptyParam, lcid);
      WorkBook:= XL.ActiveWorkbook;
    end;

    SheetCount:= WorkBook.Sheets.Count;
    if not (SheetCount>1) then
      while SheetCount<2 do begin    // Добавим один новый лист после текущего
        XL.ActiveWorkbook.Sheets.Add(EmptyParam, WorkBook.Sheets.Item[WorkBook.Sheets.Count], 1, xlWorksheet, lcid);
        SheetCount:= WorkBook.Sheets.Count;
      end; 
    SheetID:= 2;
    SheetName:= 'Роли';
    WorkSheet:= WorkBook.Sheets.Item[SheetID] as Excel_TLB._Worksheet;
    WorkSheet.Name:= fnValidExcelSheetName(SheetName);
    SheetID:= 1;
    SheetName:= 'Отчеты';
    WorkSheet:= WorkBook.Sheets.Item[SheetID] as Excel_TLB._Worksheet;
    WorkSheet.Name:= fnValidExcelSheetName(SheetName);
    j:= 1;
    i:= 1;
    Title:= 'Отчет об использовании отчетов и импортов за период с '+DateToStr(DateStart)+' по '+ DateToStr(DateEnd);
    usedRng:= WorkSheet.Range['A'+IntToStr(j), EmptyParam];
    usedRng.Value2:= Title;
    inc(j);
    usedRng:= WorkSheet.Range['A'+IntToStr(j), EmptyParam];
    usedRng.Value2:= '№';
    usedRng:= WorkSheet.Range['B'+IntToStr(j), EmptyParam];
    usedRng.Value2:= 'Наименование отчета/импорта';
    usedRng:= WorkSheet.Range['C'+IntToStr(j), EmptyParam];
    usedRng.Value2:= 'Кол-во отчетов';
    usedRng:= WorkSheet.Range['D'+IntToStr(j), EmptyParam];
    usedRng.Value2:= 'Кол-во импортов';
    usedRng:= WorkSheet.Range['E'+IntToStr(j), EmptyParam];
    usedRng.Value2:= 'Роль';
    inc(j);
    if not IBSQL.EOF then begin
      ROLECODE:= IBSQL.FieldByName('ROLECODE').AsInteger;
      TestCssStopException;
      IBSQL.Next;
    end;
    Percent:= 85/ROLECODE;                        
    ROLECODE:= 0;
    while not IBSQL.EOF do begin
      try
        WorkSheet:= WorkBook.Sheets.Item[1] as Excel_TLB._Worksheet;
        ROLENAME:= IBSQL.FieldByName('ROLENAME').AsString;
        ROLECODE:= IBSQL.FieldByName('ROLECODE').AsInteger;
        if IMPCODE<> IBSQL.FieldByName('IMTPCODE').AsInteger then begin
          ImpCheck.SetProcessPercent(UserId,ThreadData.ID,Percent);
          countI:= 0; 
          countR:= 0;
          IMPCODE:= IBSQL.FieldByName('IMTPCODE').AsInteger;
          IMPNAME:= IBSQL.FieldByName('IMTPNAME').AsString;
          REPORT:= IBSQL.FieldByName('IMTPREPORT').AsString;
          IMPORT:= IBSQL.FieldByName('IMTPIMPORT').AsString;
          TypeImp:= IntToStr(IMPCODE);
          LogIBSQL.SQL.Text:= textCount+TypeImp+'"';
          LogIBSQL.ParamByName('pDateBeg').AsDateTime:= DateStart;
          LogIBSQL.ParamByName('pDateEnd').AsDateTime:= DateEnd+1;
          if REPORT='T' then begin
            LogIBSQL.ParamByName('pTHLGCOMMAND').AsInteger:= csGetBaseStamp;
            LogIBSQL.ExecQuery;
            countR:=  LogIBSQL.FieldByName('countQ').AsInteger;
            LogIBSQL.Close;
          end;
          if IMPORT='T' then begin
            LogIBSQL.ParamByName('pTHLGCOMMAND').AsInteger:= csCommonImport;
            LogIBSQL.ExecQuery;
            countI:=  LogIBSQL.FieldByName('countQ').AsInteger;
            LogIBSQL.Close;
          end;
          usedRng:= WorkSheet.Range['A'+IntToStr(j), EmptyParam];
          usedRng.Value2:= IntToStr(IMPCODE);
          usedRng:= WorkSheet.Range['B'+IntToStr(j), EmptyParam];
          usedRng.Value2:= IMPNAME;
          usedRng:= WorkSheet.Range['C'+IntToStr(j), EmptyParam];
          usedRng.Value2:= IntToStr(countR);
          usedRng:= WorkSheet.Range['D'+IntToStr(j), EmptyParam];
          usedRng.Value2:= IntToStr(countI);
          usedRng:= WorkSheet.Range['E'+IntToStr(j), EmptyParam];
          if ROLENAME<>'' then
            usedRng.Value2:= ROLENAME;
          inc(j);
          inc(iCount);
        end
        else begin
          usedRng:= WorkSheet.Range['E'+IntToStr(j), EmptyParam];
          if ROLENAME<>'' then
            usedRng.Value2:= ROLENAME;
          inc(j);
          inc(iCount)
        end;
        SheetName:= 'Роли';
        k:= SLRole.IndexOf(ROLENAME);
        if ROLENAME<>'' then
        if k >-1 then
          WorkSheet.Hyperlinks.Add(usedRng,'#'''+SheetName+'''!'+
              'A'+IntToStr(integer(SLRole.Objects[k])), EmptyParam, EmptyParam, EmptyParam)
        else begin
          SLRole.AddObject(ROLENAME,Pointer(i));
          WorkSheet.Hyperlinks.Add(usedRng,'#'''+SheetName+'''!'+
                'A'+IntToStr(i), EmptyParam, EmptyParam, EmptyParam);
          WorkSheet:= WorkBook.Sheets.Item[2] as Excel_TLB._Worksheet;
          usedRng:= WorkSheet.Range['A'+IntToStr(i), EmptyParam];
          usedRng.Value2:= ROLENAME;
          IBSQLEmpl.ParamByName('pEMRLROLECODE').AsInteger:= ROLECODE;
          IBSQLEmpl.ExecQuery;        
          while not IBSQLEmpl.EOF do begin
            try
              EMRLCODE:= IBSQLEmpl.FieldByName('EMRLEMPLCODE').AsInteger;
              if cache.EmplExist(EMRLCODE) then begin
                EmplName:= cache.arEmplInfo[EMRLCODE].EmplShortName ;
                usedRng:= WorkSheet.Range['B'+IntToStr(i), EmptyParam];
                usedRng.Value2:= EmplName;
                inc(i);
              end;
            except
              on E: Exception do begin
                prMessageLOGS(nmProc+' '+'Ошибка при обработке результатов запроса. IBSQLEmpl: '+ E.Message, 'import', false) ;
              end;
            end;
            TestCssStopException;
            IBSQLEmpl.Next;  
          end;
          IBSQLEmpl.Close;
        end;
      except
        on E: Exception do begin
          prMessageLOGS(nmProc+' '+'Ошибка при обработке результатов запроса. IBSQL: '+ E.Message, 'import', false) ;
      
        end;
      end;
      prStopProcess( UserID, ThreadData.ID);
      TestCssStopException;
      IBSQL.Next;
    end;
    WorkSheet.Activate(lcid);
  except
    on E: EBOBError do begin
      s:= E.Message;
//    raise EBOBError.Create(Title+#10+'  '+s);
    end;
  end;
  finally
    Workbook.CheckCompatibility:= False;
    WorkBook.SaveCopyAs(FName, lcid);
    WorkBook.Close(false, EmptyParam, EmptyParam, lcid);
    XL.Quit;// закрыть Excel
    XL.Disconnect;
    CoUnInitialize;
    prFree(XL);
    prFree(SLRole);
    prFree(FilterData);
    SetLength(MsgStr, 0); 
    if iCount>0 then begin
      Title:= 'Отчет об использовании отчетов и импортов за период с'+DateToStr(DateStart)+' по '+ DateToStr(DateEnd);   
      Subj:= 'Отчет 66 '+FormatDateTime(cDateTimeFormatY2S, Now());
      ContentType:= XMLContentType;// for Excel
      lstBodyMail:= TStringList.Create;
      lstBodyMail.Add(Title); 
    end;
    if (iCount=0) and (s='') then begin
      s:= MessText(mtkNotFoundDataUse);
//      raise Exception.Create(Title+#10+'  '+s);
    end;  
    result:= s;
    prFreeIBSQL(LogIBSQL);
    if Pool<>nil then Pool.SetFreeCnt(LogIBDatabase, True);
    prFreeIBSQL(IBSQL);
    prFreeIBSQL(IBSQLEmpl);
    cntsORD.SetFreeCnt(IBDatabase);
  end;
end;
