                       // проверка необходимых параметров и доступности операции
    if (OpCode in [resAdded, resNotWrong, resWrong]) and (puserID<1) then begin
      //raise Exception.Create(MessText(mtkNotValidParam)+' юзера')
      ss:= ss+ MessText(mtkNotValidParam)+' юзера';
      flErr:= true;
    end
    else if (OpCode in [resAdded]) and (srcID<1) then begin
      //raise Exception.Create(MessText(mtkNotValidParam)+' источника')
      ss:= ss+ MessText(mtkNotValidParam)+' источника';
      flErr:= true;
    end;
    if CheckNotValidUser(puserID, isWe, s) then begin
      //raise EBOBError.Create(s); // проверка юзера
