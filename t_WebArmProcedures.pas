         if pos(country,gadr)>0 then begin
           result:= gadr;
         end;
       except
         on E: Exception do begin
            prMessageLOGS('Ошибка fnGetGeoParamsGoogle:'+' '+ E.Message, 'import', true) ;
            prMessageLOGS('Адрес:'+' '+ adr, 'import', true) ;
          end;
       end;
     finally
       prFree(HTTP);
       prFree(pIniFile);
     end;
   end;
