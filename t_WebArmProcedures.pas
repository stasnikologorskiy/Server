      //ImpCheck.SetProcessPercent(UserId,ThreadData.ID,Percent);
//prMessageLOGS('Время всего'+': - '+
//        FloatToStr(RoundTo((Now-timerStart)*60*60*24,-5))+' сек','Log_prg.log' , false); //66% 
//timer:= now;  
end; 
