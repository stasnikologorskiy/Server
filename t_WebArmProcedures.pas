            WareList:= (MWareList.Count>0);
          end;
          MWareList.Clear; 
          prStopProcess( UserID, ThreadData.ID);         
          if WareList then begin
            ss:= IBQuery.FieldByName('RN_REF_NR').AsString+';'+ IBQuery.FieldByName('RN_MF_ID').AsString+';'+ 
