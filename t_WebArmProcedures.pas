function GetAddres(var s: string; var sMail: string): string;
var mail, xyz: string;
    p: integer;
begin
  result:= '';
  xyz:= '';
  while pos(',',s)>0 do begin
    p:= pos(',',s);
    mail:= trim(copy(s,1,p-1));
    if mail=cFictiveEmail then  xyz:= xyz+fnIfStr(length(result)>0,',','')+ ' Фиктивный адрес: '+mail
    else
    if not fnCheckEmail(mail) then result:= result+fnIfStr(length(result)>0,',','')+ mail
    else 
      if pos(mail, sMail)=0 then 
        sMail:= sMail+fnIfStr(length(sMail)>0,',','')+mail;
    s:= copy(s, p+1,length(s));
  end;
  if (length(s)<1) or not fnCheckEmail(s) then
    result:= result+fnIfStr(length(result)>0,',','')+ s
  else
    if (pos(s, sMail)=0) then begin
      if s=cFictiveEmail then  xyz:= xyz+fnIfStr(length(result)>0,',','')+ ' Фиктивный адрес: '+s
      else 
        sMail:= sMail+fnIfStr(length(sMail)>0,',','')+s;
    end;
  s:= xyz;
end;

