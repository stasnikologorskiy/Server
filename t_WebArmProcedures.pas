                      if AWare.TypeID<>Ware.TypeID then ss:= 'Внимание!Несоответствие типов! Тип '+ AWare.TypeName+' и тип '+Ware.TypeName
                      else ss:='';
                    
                      NotAWareList.AddObject(ATDWareList[i]+';'+Ware.Name+';'+arRec[0]+';'+Manager+';'+ss,pointer(Ware.ID));
