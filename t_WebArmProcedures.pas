const nmProc = 'prStopIEOperation'; // имя процедуры/функции
var
  UserId, FirmID, ThreadDataID: integer;
  errmess: string;

begin
  Stream.Position:= 0;
  try
    UserID       := Stream.ReadInt;
    ThreadDataID := Stream.ReadInt;
