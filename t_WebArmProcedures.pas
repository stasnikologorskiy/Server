            if ModelsLists[k]=nil then ModelsLists[k]:= TStringList.Create;
          //перевод указателя на последнюю используемую ячейку листа
            usedRng:= WorkSheet.Range['A1', EmptyParam].SpecialCells(xlCellTypeLastCell,EmptyParam);
