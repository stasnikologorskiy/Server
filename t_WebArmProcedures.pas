      LogIBSQL.SQL.Add('FROM LOGTHREADS left join  IPLOGUSERS on IPLUCODE=THLGUSERIP');
      LogIBSQL.SQL.Add('where THLGBEGINTIME between :pDateStart and :pDateEnd and not THLGUSERIP is null and not THLGFIRMID is null ');
