      if not Assigned(IBD) or not IBD.Connected then begin      
        prFreeIBSQL(IBQuery);                                   
        if Assigned(IBD) then cntsTDT.SetFreeCnt(IBD, True);    

        IBD:= cntsTDT.GetFreeCnt;
          IBQuery:= fnCreateNewIBSQL(IBD, 'IBQuery_'+nmProc, -1, tpRead, true);
