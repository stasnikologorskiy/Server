//ErrPos:=2;                                     
      try  //2
        SetLength(ErrMsg,0);
        SetLength(LogStr,0);
        lstBodyMail:= TStringList.Create;
        lstBodyMailMes:= TStringList.Create;
        SubjMes:='';
