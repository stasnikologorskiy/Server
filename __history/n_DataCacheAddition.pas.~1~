  private
    FTypeSys  : Byte;            // Тип системы 1 - Авто, 2 - Мото and etc.
    FNodesList: TStringList;     // последовательный список нод дерева, Object - Pointer TAutoTreeNode
    FNodeItems: array of TAutoTreeNode; // список узлов дерева по кодам, FNodeItems[0] - корневой узел дерева
