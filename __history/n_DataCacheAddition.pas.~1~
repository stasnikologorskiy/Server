//------------------------------------------------------ Дерево узлов автомобиля
  TAutoTreeNode = Class (TSubVisDirItem)  // Узел дерева
  private // FID - Код узла, FName - Наименование, State - Статус, FSubCode - Код TecDoc,
          // FOrderNum - порядк.номер ноды в дереве своей системы
    FMainCode: Integer;         // код TRNACODE главной ноды
    FMeasID  : Byte;            // Код ед.изм.
    FTypeSys : Byte;            // Тип системы 1 - Авто, 2 - Мото and etc.
    FNameSys : String;          // Наименование системное
    FChildren: TStringList;     // Список подузлов, Object - Pointer TAutoTreeNode
    function GetIsEnding: boolean;
  public
    constructor Create(pID, pParentID, pMeasID, pSysID: Integer;
                pName, pNameSys: String; pMainCode: Integer=0; pCodeTD: Integer=0;
                pIsGATD: Boolean=False; pVisible: Boolean=True);
//                pIsGATD: Boolean=False; pVisible: Boolean=True; pIsEng: Boolean=False);
    destructor Destroy; override;
    property Children: TStringList read FChildren; // Список подузлов, Object - Pointer TAutoTreeNode
    property NameSys : String  read FNameSys;      // Наименование системное
    property ParentID: Integer read FParCode;      // Код родителя
    property MainCode: Integer read FMainCode write FMainCode; // код TRNACODE главной ноды из повторяющихся
    property OrderNum: Integer read FOrderNum write FOrderNum;  // порядк.номер ноды в дереве своей системы
    property MeasID  : Byte    read FMeasID;       // Код ед.изм.
    property TypeSys : Byte    read FTypeSys;      // Тип системы 1 - Авто, 2 - Мото and etc.
    property Visible : Boolean index ik8_2 read GetDirBool write SetDirBool; // признак видимости ноды
    property IsGATD  : Boolean index ik8_3 read GetDirBool write SetDirBool; // признак GA TecDoc (конечной ноды авто)
//    property IsEng   : Boolean index ik8_4 read GetDirBool write SetDirBool; // признак ноды двигателя
    property IsEnding: Boolean read GetIsEnding; // признак конечной ноды
  end;

{
  TTreeNodes = Class (TDirItems)         // Дерево узлов
  private // FItems - массив ссылок на узлы дерева, FItems[0] - ссылка на корневой узел дерева
    FSysNodesLists: TArraySysTypeLists;  // списки нод дерева по системам учета последовательные по всем ветвям (Objects-Pointer TAutoTreeNode)
    function GetNodeByID(pID: Integer): TAutoTreeNode;

  public
