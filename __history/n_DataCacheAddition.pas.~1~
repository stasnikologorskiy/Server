{   Модуль дополнения к DataCacheInMemory для работы с оригинальными номерами и не только
{ ------------------------------------------------------------------------------------- }
unit n_DataCacheAddition;

interface

uses Classes, Types, SysUtils, IBDataBase, IBQuery, IBSQL, Math, Forms, SyncObjs,
     Contnrs, DateUtils, n_free_functions, v_constants, v_DataTrans,
     n_constants, n_DataSetsManager, n_LogThreads, n_DataCacheObjects;

type
//---------------------------------------------- набор списков по системам учета
  TArraySysTypeLists = Class (TArrayTypeLists)
  public
    constructor Create(fSorted: Boolean=False; fDelimiter: Boolean=False);
  end;

//--------------------------------------------------------------------- связка 2
  TSecondLink = class (TLinkLink) // LinkPtr - ссылка на узел дерева
  protected                       // DoubleLinks - связка 3 (связки 2 с товарами)
    FQty: Single;           // количество
  public
    constructor Create(pSrcID: Integer; pQty: Single; pNodePtr: Pointer=nil;
                pIsLink: Boolean=False; pHasWares: Boolean=False; pHasFilters: Boolean=False);
    property IsLinkNode    : boolean index ik8_3 read GetLinkBool write SetLinkBool; // признак узла связки 2 (конечная нода)
    property NodeHasWares  : boolean index ik8_4 read GetLinkBool write SetLinkBool; // признак наличия товаров в узле или его ветке
    property NodeHasFilters: boolean index ik8_5 read GetLinkBool write SetLinkBool; // признак наличия фильтров в узле модели
    property Qty: Single read FQty write FQty;       // количество
    function GetWareCodes: Tai;                      // must Free, массив кодов товаров связки 2
  end;

//------------------------------------------------ Модель автомобиля / мотоцикла
  TModelParams = Class  // параметры модели
  public
    pCCM       : Word;     // Тех. обьем куб.см.
    pKW        : Word;     // Мощность кВт
    pHP        : Word;     // Мощность ЛС
    pYStart    : Word;     // Год начала выпуска
    pYEnd      : Word;     // Год окончания выпуска
    pMStart    : Byte;     // Месяц начала выпуска
    pMEnd      : Byte;     // Месяц окончания выпуска
    pCylinders : Byte;     // Количество цилиндров
    pValves    : Byte;     // Количество клапанов на одну камеру сгорания
    pBodyID    : Byte;     // Код, тип кузова               (TYPEDIR=1)
    pDriveID   : Byte;     // Код, тип привода              (TYPEDIR=2)
    pEngTypeID : Byte;     // Код, тип двигателя            (TYPEDIR=3)
    pFuelID    : Byte;     // Код, тип топлива              (TYPEDIR=4)
    pFuelSupID : Byte;     // Код, система впрыска          (TYPEDIR=5)
    pBrakeID   : Byte;     // Код, тип тормозной системы    (TYPEDIR=6)
    pBrakeSysID: Byte;     // Код, Тормозная система        (TYPEDIR=7)
    pCatalID   : Byte;     // Код, Тип катализатора         (TYPEDIR=8)
    pTransID   : Byte;     // Код, Вид коробки передач      (TYPEDIR=9)
  end;

  TNodeLinks = Class;

  TModelAuto = Class (TSubVisDirItem)  // модель
  private // FID - Код модели, FName - Название модели,  FSubCode - Код TecDoc, FOrderNum - порядк.№ модели,
          // IsVisible - Признак видимости модели, FLinks - связки с маркировками двигателей, FParCode - Код модельного ряда
    FParams       : TModelParams; // параметры модели
    FNodeLinks    : TNodeLinks;   // связки 2 узлов дерева с моделью, Links.GetDoubleLinks(NodeID) - связки 3 товаров со связкой 2
    function GetIntM(const ik: T8InfoKinds): Integer;              // получить код
    procedure SetIntM(const ik: T8InfoKinds; Value: Integer);      // записать код
    function GetStrM(const ik: T8InfoKinds): String;               // получить строку
    function GetNodesExists: Boolean;                         //  наличие связок 2
    procedure ClearLinks;
  public
    CS_mlinks: TCriticalSection; // для линков
    constructor Create(pModelID, pModelTDcode, pOrdNum, pModelLineID: Integer; pName: String);
    destructor Destroy; override;
    function GetModelNodesLinks: TLinks; // must Free , список связок 2 модели из базы
    function GetModelNodesList(OnlyVisible: boolean=False; flFromBase: boolean=False): TList; // must Free, список связок 2 с узлами модели (OnlyVisible=True - только видимые ноды)
    function GetModelNodeIsSecondLink(nodeID: Integer): Boolean;   // признак узла 2-й связки
    function GetModelNodeWaresList(nodeID: Integer; withChildNodes: boolean=True; flFromBase: boolean=False): TStringList; // must Free, сорт.список товаров по узлу (Object - Pointer(ID))
    function GetModelNodeWares(nodeID: Integer; withChildNodes: boolean=True; flFromBase: boolean=False): Tai;             // must Free, массив кодов товаров по узлу
    function GetModelParamsUpdSql(mps: TModelParams): string;
    function SetModelVisible(isVis: Boolean): String;          // Изменить видимость модели
    function CheckDiffModelMarks(marks: TStringList): Boolean; // проверить маркировки по marks: True - есть различия
    function ModelEdit(pName: String; pVisible, pIsTop: Boolean; pUserID: Integer; mps: TModelParams;
                       pOrdNum: Integer=-1; pTDcode: Integer=-1; marks: TStringList=nil): String;
    procedure SetModelParams(mps: TModelParams; flFill: Boolean=False);
    procedure SetModelMarks(marks: TStringList; pUserID: integer); // записать маркировки по marks
    function AddModelEngLink(pEngID, pUserID: Integer): String;  // Добавить связку модели с маркировкой двигателя

    property ModelLineID   : Integer index ik8_3 read GetIntM  write SetIntM;  // Код модельного ряда
    property ModelOrderNum : Integer index ik8_4 read GetIntM  write SetIntM;  // порядк.№ модели
    property TypeSys       : Integer index ik8_1 read GetIntM;  // Тип системы 1 - Авто, 2 - Мото and etc.
    property ModelMfauID   : Integer index ik8_2 read GetIntM;  // Код производителя
    property ModelHasWares : Boolean index ik8_3 read GetDirBool write SetDirBool; // Признак наличия товаров по модели
    property IsTop         : Boolean index ik8_4 read GetDirBool write SetDirBool; // Топ модель
    property NodesExists   : Boolean read GetNodesExists;     // наличие связок 2
    property SortName      : String  index ik8_1 read GetStrM;  // Наименование модели для сортировки
    property MarksCommaText: String  index ik8_2 read GetStrM;  // список маркировок двигателей
    property ModelLineName : String  index ik8_3 read GetStrM;  // Название модельного ряда модели
    property ModelMfauName : String  index ik8_4 read GetStrM;  // Название производителя модели
    property WebName       : String  index ik8_5 read GetStrM;  // Название модели для вывода в Web
    property Params        : TModelParams read FParams;       // параметры модели
    property NodeLinks     : TNodeLinks   read FNodeLinks;    // связки 2 узлов дерева с моделью
    property EngLinks      : TLinks       read FLinks;        // связки с маркировками двигателей
  end;

  TModelsAuto = Class   // Список моделей
  private
    FarModels: array of TModelAuto; // массив моделей (индекс - ID)
    CS_Models: TCriticalSection; // для моделей
  public
    constructor Create;
    destructor Destroy; override;
    function GetModel(pModelID: Integer): TModelAuto;       // Получить ссылку на обьект модели
    function ModelExists(pModelID: Integer): Boolean;       // Проверка существования модели
    function GetMLModelsList(pModelLineID: Integer): Tai;   // must Free, Возвращает массив кодов моделей указанного модельного ряда
    function ModelAdd(var pModelID: Integer; pName: String; // Добавить модель, с занесением в базу
             pVis, pTops: Boolean; UserID, MLineID: Integer; mps: TModelParams;
             pOrdNum: Integer=-1; pTDcode: Integer=0; marks: TStringList=nil): String;
    function ModelDel(pModelID: Integer): String;           // Удалить модель, с удалением из базы
    procedure SetStates(pState: Boolean; pModelLineID: Integer=0);     // устанавливает флаг проверки всем моделям модельного ряда
    function SaveMarkAndGetID(var pMarkID: Integer; pTDcode, pModelID, UserID: Integer; pName: String): String; // Добавить маркировку в базу
    property Items[index: Integer]: TModelAuto read GetModel; default; // Безопасное получение ссылки на обьект модели
  end;

//-------------------------------------------------------------------- Двигатели
  TEngParams = Class  // параметры двигателя
  public
    pCompFrom   : Integer; // Компрессор * 100 от
    pCompTo     : Integer; // Компрессор * 100 до
    pRPMtorqFrom: Integer; // Момент вращения (Nm) при [об/мин] от
    pRPMtorqTo  : Integer; // Момент вращения (Nm) при [об/мин] до
    pBore       : Integer; // Расточка * 1000
    pStroke     : Integer; // Ход поршня * 1000
    pYearFrom   : Word;    // Год выпуска от
    pYearTo     : Word;    // Год выпуска до
    pKWfrom     : Word;    // Мощность кВт от
    pRPMKWfrom  : Word;    // при [об/мин] от
    pKWto       : Word;    // Мощность кВт до
    pRPMKWto    : Word;    // при [об/мин] до
    pHPfrom     : Word;    // Мощность ЛС от
    pHPto       : Word;    // Мощность ЛС до
    pCCtecFrom  : Word;    // Тех.обьем в куб.см. от
    pCCtecTo    : Word;    // Тех.обьем в куб.см. до
    pMonFrom    : Byte;    // Месяц выпуска от
    pMonTo      : Byte;    // Месяц выпуска до
    pVal        : Byte;    // Количество клапанов
    pCyl        : Byte;    // Количество цилиндров
    pCrank      : Byte;    // Кол-во подшипников коленвала
    pDesign     : Byte;    // Код, Исполнение двигателя   (KT 96) (TYPEDIR=13)
    pFuelType   : Byte;    // Код, Вид топлива            (KT 88) (TYPEDIR=12)
    pFuelMixt   : Byte;    // Код, Система впрыска        (KT 97) (TYPEDIR=5)
    pAspir      : Byte;    // Код, Подача воздуха         (KT 99) (TYPEDIR=14)
    pType       : Byte;    // Код, Вид двигателя          (KT 80) (TYPEDIR=3)
    pNorm       : Byte;    // Код, Норма выхлопных газов  (KT 63) (TYPEDIR=15)
    pCylDesign  : Byte;    // Код, Тип привода ГРМ        (KT 79) (TYPEDIR=16)
    pManag      : Byte;    // Код, Привод распредвала     (KT 77) (TYPEDIR=17)
    pValCnt     : Byte;    // Код, Привод клапана         (KT 78) (TYPEDIR=18)
    pCoolType   : Byte;    // Код, Тип системы охлаждения (KT 76) (TYPEDIR=19)
    pSalesDesc  : String;  // Обозначение продажи
    procedure Clear;       // очистить параметры
  end;

  TEngine = Class (TSubDirItem) // двигатель
  protected // FID - код, FName - маркировка, FSubCode - код TecDoc
    FEngMFau: Integer;    // код производителя
    FParams : TEngParams; // параметры двигателя
    function GetStrEng(const ik: T8InfoKinds): String;        // получить строку
  public
    constructor Create(pID, pSubCode, pEngMFau: Integer; pName: String; WithLinks: Boolean=False);
    destructor Destroy; override;
    procedure SetParams(eps: TEngParams; flFill: Boolean=False); // записать / обновить параметры
    function GetViewList(Delim: String=cSpecDelim): TStringList;   // must Free, список параметров двигателя для просмотра
    function GetNodesLinks: TLinks;   // must Free, список связок 2 двигателя
//    function GetEngNodeWaresList(nodeID: Integer; withChildNodes: boolean=True): TStringList; // must Free, список товаров по узлу двигателя
//    function GetEngNodeWares(nodeID: Integer; withChildNodes: boolean=True): Tai;             // must Free, массив кодов товаров по узлу двигателя
    function GetEngNodeWareUsesView(nodeID: Integer; WareCodes: Tai; sFilters: String=''): TStringList; // must Free, список условий по товарам узла двигателя для просмотра

    function GetEngNodeWaresWithUsesByFilters(NodeID: Integer; // фильтр.список товаров с текстами и условиями к связкам 3, Objects - WareID
             withChildNodes: boolean=True; sFilters: String=''): TStringList;  // must Free, sFilters - коды значений критериев через запятую

    property TDCode   : Integer read FSubCode;                // код TecDoc
    property EngMFau  : Integer read FEngMFau write FEngMFau; // код производителя
    property EngParams: TEngParams read FParams;              // параметры двигателя
    property Mark     : String  index ik8_1 read GetStrEng;     // маркировка
    property EngKWstr : String  index ik8_2 read GetStrEng;     // строка - значение Мощность кВт
    property EngHPstr : String  index ik8_3 read GetStrEng;     // строка - значение Мощность ЛС
    property EngCCstr : String  index ik8_4 read GetStrEng;     // строка - значение Тех.обьем в куб.см.
    property EngCYLstr: String  index ik8_5 read GetStrEng;     // строка - значение Количество цилиндров
    property WebName  : String  index ik8_6 read GetStrEng;     // WebName
    property MfauName : String  index ik8_7 read GetStrEng;     // MfauName
    property EngHasNodes: boolean index ik8_3 read GetDirBool write SetDirBool; // признак наличия 2-х связок
    property EngHasWares: boolean index ik8_4 read GetDirBool write SetDirBool; // признак наличия 3-х связок
  end;

  TEngines = Class (TDirItems) // двигатели
  public
    function GetEngine(engID: integer): TEngine;         // получить двигатель по коду
    function FindEngineByTDcode(engTD: integer; var eng: TEngine): Boolean; // найти двигатель по коду TecDoc
    function AddEngine(var pID: Integer; pSys, pTDcode, pMFau, pUserID: Integer; // добавить двигатель
                       pMark: String; eps: TEngParams): String;
    function EditEngine(pID: Integer; pTDcode, pMFau, pUserID: Integer;          // изменить двигатель
                       pMark: String; eps: TEngParams): String;
    function GetMfauEngList(mfau: integer): TStringList; // must Free, сорт.по маркировкам список двигателей производителя (Object-TEngine)
  end;

//---------------------------------------------------------------- Модельный ряд
  TModelLine = Class (TSubVisDirItem)
  private // FID, FName - Код, Наименование модельного ряда, State - Статус проверки,
          // FSubCode - Код TecDoc, { FLinks - Сортированный список линков на модели модельного ряда / заготовка}
          // IsVisible - Признак видимости модельного ряда, FParCode - Код производителя авто
    FYStart    : Word;      // Год начала производства
    FYEnd      : Word;      // Год окончания производства
    FMStart    : Byte;      // Месяц начала производства
    FMEnd      : Byte;      // Месяц окончания производства
    FTypeSys   : Byte;      // Тип системы 1 - Авто, 2 - Мото and etc.
    FMLModelsSort : TStringList; // Сортированный список моделей модельного ряда   (Object - Pointer(ID))
    FMLModelsTopUp: TStringList; // список моделей модельного ряда с топами вверху (Object - Pointer(ID))
    function GetIntML(const ik: T8InfoKinds): Integer;              // получить код
    procedure SetIntML(const ik: T8InfoKinds; Value: Integer);      // записать код
    function GetStrML(const ik: T8InfoKinds): String;               // получить строку
    function GetHasVisModels: Boolean;
  public
    constructor Create(pMLineID, pTDcode, pMFAID: Integer; pName: String);
    destructor Destroy; override;
    procedure ModelDelFromLine(pModelID: Integer); // Удалить модель из модельного ряда  из кеш
    procedure CheckModelsLists;                   // заполнить списки моделей модельного ряда
    function GetListModels(pTopsUp: Boolean=False): TStringList; // Получить список моделей, (Object - Pointer(ID))
    function GetMLModelIDByTDcode(pModTDnr: Integer): Integer; // получить код модели модельного ряда по коду TecDoc
    property MFAID       : Integer index ik8_2 read GetIntML write SetIntML; // Код производителя авто
    property ModelsCount : Integer index ik8_1 read GetIntML;  // количество моделей модельного ряда
    property YStart      : Integer index ik8_3 read GetIntML;  // Год начала производства
    property YEnd        : Integer index ik8_4 read GetIntML;  // Год окончания производства
    property MStart      : Integer index ik8_5 read GetIntML;  // Месяц начала производства
    property MEnd        : Integer index ik8_6 read GetIntML;  // Месяц окончания производства
    property TypeSys     : Integer index ik8_7 read GetIntML;  // Тип системы 1 - Авто, 2 - Мото and etc.
    property SortName    : String  index ik8_1 read GetStrML;  // Наименование модельного ряда для сортировки
    property MLHasWares  : Boolean index ik8_3 read GetDirBool write SetDirBool; // Признак наличия товаров по модельному ряду
    property IsTop       : Boolean index ik8_4 read GetDirBool write SetDirBool; // Топ
    property HasVisModels: Boolean read GetHasVisModels;     // признак наличия видимых моделей модельного ряда
  end;

  TModelLines = Class    // Список модельных рядов
  private
    FarModelLines: array of TModelLine;    // массив модельных рядов (индекс - ID)
  public
    CS_MLines: TCriticalSection; // для мод.рядов
    constructor Create;
    destructor Destroy; override;
    function GetModelLine(pModelLineID: Integer): TModelLine; // Получить ссылку на модельный ряд по коду
    function ModelLineExists(pID: Integer): Boolean; // Проверка существования модельного ряда
    function GetManufSysModelLinesList(pManufID, pTypeSys: Integer): TStringList; // must Free, список модельных рядов производителя по указанной системе (Object - Pointer(ID))
    procedure SetStates(pState: Boolean; pTypeSys: Integer=0);   // устанавливает флаг проверки всем элементам
    property Items[Index: Integer]: TModelLine read GetModelLine; default; // Безопасное получение ссылки на обьект модельного ряда
  end;

//-------------------------------------------------------- Производитель техники
  TManufacturer = Class (TSubDirItem)
  private // FID, FName, State - Код, Наименование производителя, Статус, FSubCode - Код TecDoc
    FManufSysMLsSort : TArraySysTypeLists; // Сортированные списки модельных рядов по системам учета, (Object - Pointer(ID))
    FManufSysMLsTopUp: TArraySysTypeLists; // списки модельных рядов с топами вверху по системам учета, (Object - Pointer(ID))
    FManufSearchONlist: TStringList;       // Сортированный список ориг.номеров производителя без спец/симв, (Object - Pointer(ID))
    FMfauOpts: set of T8InfoKinds;
    function GetBoolMf(ik: T8InfoKinds): boolean;        // получить признак
    procedure SetBoolMf(ik: T8InfoKinds; Value: boolean); // записать признак
  public
    constructor Create(pID: Integer; pName: String; pTDnr: Integer=0);  // Код, Имя, Код TecDoc
    destructor Destroy; override;
    procedure TopsSet(pTypeSys: Integer; IsTop: Boolean);    // Установить / снять ТОПовость
    procedure TypeSysSet(pTypeSys: Integer; IsSys: Boolean); // Установить / снять принадлежность системе
    procedure VisibleSet(pTypeSys: Integer; IsVis: Boolean); // Установить / снять видимость
    function CheckIsTypeSys(pTypeSys: Integer): Boolean;    // Проверка принадлежности производителя к системе Авто/ Мото
    function CheckIsVisible(pTypeSys: Integer): Boolean;    // Проверить видимость
    function CheckIsTop(pTypeSys: Integer): Boolean;        // Проверить производитель ТОПОВЫЙ для системы учета
    function CheckOtherTypeSys(pTypeSys: Integer): Boolean; // Проверка принадлежности к какой-нибудь системе кроме pTypeSys
    function CheckHasModelLines(pTypeSys: Integer = 0): Boolean;  // Проверка имеютеся ли у производителя модельные ряды
    procedure CheckModelLinesLists(pTypeSys: Integer);   // заполнить Списки модельных рядов производителя по выбранной системе
    function GetModelLinesList(pTypeSys: Integer; pTopsUp: Boolean): TStringList; // Список модельных рядов производителя по выбранной системе, сортированный по наименованию  (Object - Pointer(ID))
    function GetModelsList(pTypeSys: Integer): TStringList; // must Free, Список моделей производителя по выбранной системе (Object - Pointer(ID))
    function ModelLineAdd(var ModelLineID: Integer; pName: String; // Добавить модельный ряд в кеш и в базу к производителю к системе
             pTypeSys, pMS, pYS, pME, pYE, pUserID: Integer; pIsTop, pIsVis: Boolean; pMLTD: Integer=0): String;
    function ModelLineDel(pModelLineID: Integer): String;  // Удалить модельный ряд из кеша и из базы у производителя
    function ModelLineEdit(pModelLineID, pYS, pMS, pYE, pME, pUserID: Integer;
             pIsTop, pIsVis: Boolean; pName: String=''; pMLTD: Integer=0): String;
    function HasVisModelLines(pTypeSys: Integer): Boolean; // признак наличия видимых модельных рядов по выбранной системе
    function HasVisMLModels(pTypeSys: Integer): Boolean;   // признак наличия видимых моделей по выбранной системе
    function GetMfMLineIDByTDcode(pmlTDnr: Integer): Integer; // получить код модельного ряда авто по коду TecDoc
    property IsMF         : Boolean index ik8_1 read GetBoolMf  write SetBoolMf;  // Признак производителя
    property IsMfAUTO     : Boolean index ik8_2 read GetBoolMf  write SetBoolMf;  // Признак производителя авто
    property IsMfMOTO     : Boolean index ik8_3 read GetBoolMf  write SetBoolMf;  // Признак производителя мото
    property IsMfTopA     : Boolean index ik8_4 read GetBoolMf  write SetBoolMf;  // Признак топ-производителя авто
    property IsMfTopM     : Boolean index ik8_5 read GetBoolMf  write SetBoolMf;  // Признак топ-производителя мото
    property IsMfVisA     : Boolean index ik8_6 read GetBoolMf  write SetBoolMf;  // Признак видимости производителя авто
    property IsMfVisM     : Boolean index ik8_7 read GetBoolMf  write SetBoolMf;  // Признак видимости производителя мото
    property ManufHasWares: Boolean index ik8_3 read GetDirBool write SetDirBool; // Признак наличия товаров по производителю
    property MfHasEngWares: Boolean index ik8_4 read GetDirBool write SetDirBool; // Признак наличия товаров двигателей по производителю
    property ManufSearchONlist: TStringList read FManufSearchONlist;
  end;

  TManufacturers = Class  // Справочник производителей авто
  private
    FarManufacturers  : array of TManufacturer; // массив производителей авто (индекс - ID)
    FSysManufListSort : TArraySysTypeLists;     // Сортированные списки производителей по системам учета (Objects-Pointer TManufacturer)
