    property TypeSys : Byte    read FTypeSys;      // Тип системы 1 - Авто, 2 - Мото and etc.
    property Items[ID: Integer]: TAutoTreeNode read GetNodeByID; default;
    property NodesList: TStringList read GetNodesList;      // последовательный список нод дерева, Object - Pointer TAutoTreeNode
  end;

